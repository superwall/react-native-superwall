{
  "name": "superwall-react-native-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace SuperwallReactNativeExample.xcworkspace -scheme SuperwallReactNativeExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native/babel-preset": "^0.76.1",
    "@react-navigation/native": "7.0.14",
    "@react-navigation/stack": "7.1.1",
    "react": "18.2.0",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "2.18.0",
    "react-native-purchases": ">=7.20.2",
    "react-native-reanimated": "3.16.7",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": ">=4.0.0 <4.5.0",
    "react-native-vector-icons": "10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^15.0.0",
    "@react-native/babel-preset": "0.76.9",
    "@react-native/metro-config": "0.76.9",
    "@react-native/typescript-config": "0.76.9",
    "babel-plugin-module-resolver": "^5.0.0",
    "pod-install": "^0.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
